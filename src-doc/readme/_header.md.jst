<% if (name) { %># <%= name %><% } %><% if (meta.gemnasium) { %> [![Dependency Status](https://gemnasium.com/<%= meta.gemnasium.userId %>/<%= name %>.png)](https://gemnasium.com/<%= meta.gemnasium.userId %>/<%= name %>) <% } %><% if (meta.flattr) { if (meta.flattr.image) { if (meta.flattr.image === 'badge') { %> [![Flattr this <%= meta.flattr.category %>](https://api.flattr.com/button/flattr-badge-large.png)](https://flattr.com/submit/auto?user_id=<%= meta.flattr.userId %>&url=<%= homepage %>&title=<%= grunt.util._(name).capitalize() %>&description=<%= encodeURI(grunt.util._(description).humanize()) %>&language=en_GB&tags=<%= keywords.join(',') %>&category=<%= meta.flattr.category %>)<% }}} %>
<% if (description) { %>
> <%= description %>
<% } %>
